<bg xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
	<modal id='modal' hide-event='close-modal' clickerino-offerino-closerino="false" move-to-center='false' anchor-pos-y='15' size-delta-x='60' size-delta-y='30'>
		<vertical horizontal-fit='PreferredSize' vertical-fit='PreferredSize' spacing='3' pad='3' preferred-width='60' preferred-height='30'>
			<text id='warning-message' word-wrapping='true' align='Center' />
			<horizontal>
				<button id='yes' text='Yes' on-click='delete-confirm' click-event='close-modal' />
				<button id='no' text='No' click-event='close-modal' />
			</horizontal>
		</vertical>
	</modal>
</bg>